        "deepmerge": "^4.2.2",
        "fibers": "^4.0.2",
        "node-sass": "^7.0.0",
        "sass": "^1.23.3",
        "sass-loader": "^8.0.0",
        "typescript": "~3.5.3",
        "vue-cli-plugin-vuetify": "^2.0.1",
        "vue-template-compiler": "^2.6.10",
        "vuetify-loader": "^1.3.0"
